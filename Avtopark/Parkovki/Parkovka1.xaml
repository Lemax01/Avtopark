<Window x:Class="Avtopark.Parkovki.Parkovka1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Avtopark"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="Автопарк" Height="600" Width="1000">

    <Grid Background="#FFFCD6">
        <Image Margin="10,10,857,495" Source="/ikonka.png" />
        <Label Content="АО Турбодуй" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="154" Height="56" FontWeight="Bold" FontSize="20"/>
        <Border BorderBrush="Black" BorderThickness="1">
            <Button Margin="134,205,499,150" Click="Legkovoi">
                <Image Source="legk.png"/>
            </Button>
        </Border>

        <Button Margin="530,204,107,153" Click="Gryzovoi">
            <Image Source="gruz.png"/>
        </Button>
        <Button Content="На главную" HorizontalAlignment="Center" Margin="0,490,0,0" VerticalAlignment="Top" Width="170" Height="43" Click="Exit"/>
    </Grid>
</Window>
